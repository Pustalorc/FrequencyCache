<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net35;net40;net461;net48</TargetFrameworks>
    <RootNamespace>Pustalorc.Libraries.FrequencyCache</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageId>Pustalorc.FrequencyCache</PackageId>
    <Authors>Pustalorc</Authors>
    <Product>Pustalorc.FrequencyCache</Product>
    <Description>A cache SDK that utilizes access frequency to determine which previous items to replace within the cache.</Description>
    <NeutralLanguage>en</NeutralLanguage>
    <AssemblyName>Pustalorc.FrequencyCache</AssemblyName>
    <RepositoryType>Github</RepositoryType>
    <PackageProjectUrl>https://github.com/Pustalorc/FrequencyCache</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Pustalorc/FrequencyCache</RepositoryUrl>
    <PackageTags>cache frequency replacement access</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright Â© 2020 Pustalorc</Copyright>
    <PackageReleaseNotes>Fixes an issue causing AccessCount to be increased when updating the element in cache and when the cache update is requested.</PackageReleaseNotes>
    <Version>1.0.1</Version>
  </PropertyGroup>

</Project>
